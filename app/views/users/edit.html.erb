<div class='edit-profile-div'>
  <% if current_user == @user %>
    <%= form_for @user do |f| %>
      <%= f.text_field :first_name %> <br>
      <%= f.text_field :last_name %> <br>
      <%= f.text_field :email %> <br>
      <%= f.submit "OK" %>
    <% end %>
  <% else %>
    <h1> UNAUTHORIZED </h1>
  <% end %>
</div>
